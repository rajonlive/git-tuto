<header>
{{ section.settings.email }}
{{ section.settings.phone }}
</header>

{% schema %}
  {
    "name": "Header",
    "settings": [
      {
        "id": "email",
        "type": "text",
        "label": "Email",
        "default": "hello@shopifycanvas.com"
      },
      {
        "id": "phone",
        "type": "text",
        "label": "phone",
        "default": "0123456789"
      }
    ],
	"max_blocks": 2,
    "blocks": [
      {
        "type": "image",
        "name": "image",        
        "settings": [
          {
            "id": "content",			
            "type": "image_picker",
            "label": "Social Icon"
          }
        ]
      }
    ]
  }
{% endschema %}